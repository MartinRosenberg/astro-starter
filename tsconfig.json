{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist/"],
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"checkJs": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"paths": {
			"#*": ["src/*"]
		},
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		],
		"verbatimModuleSyntax": true
	}
}
